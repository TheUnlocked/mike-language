    "@types/chai": "^4.3.1",
    "@types/lodash": "^4.14.182",
    "@types/mocha": "^9.1.1",
    "@types/omit-deep-lodash": "^1.1.1",
    "@types/reserved-words": "^0.1.0",
    "c8": "^7.11.3",
    "chai": "^4.3.6",
    "chai-exclude": "^2.1.0",
    "mocha": "^10.0.0",
    "nodemon": "^2.0.16",
    "omit-deep-lodash": "^1.1.7",
    "rimraf": "^3.0.2",
    "ts-node": "^10.8.0",
    "typescript": "^4.9.4"
  }
