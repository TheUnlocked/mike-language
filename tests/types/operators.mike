    //        v expect $t == type('boolean')
    true || 1 == 1;

    // v expect $t == type('int')
       -(5);
    // v expect $t == type('float')
    //   v expect $t == type('float')
       -(5.0);
    // v expect $t == type('boolean')
       !true;
