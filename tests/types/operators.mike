on _() {
    //v expect $t == type('int')
    1 + 2;

    //v expect $t == type('int')
    1 * 3;
    //v expect $t == type('option<int>')
    1 / 3;
    //  v expect $t == type('float')
    1.0 / 3.0;

    //    v expect $t == type('boolean')
    1 + 1 >= 2;
    //    v expect $t == type('boolean')
    1 + 1 == 2;
    //    v expect $t == type('boolean')
    //         v expect $t == type('int')
    1 + 2 != 2 * 2;
    // v expect $t == type('boolean')
